    "@types/jquery": "^3.5.5",
    "@types/lodash": "^4.14.168",
    "auto-launch": "^5.0.5",
    "axios": "^0.21.1",
    "bootstrap": "^4.6.0",
    "cheerio": "^1.0.0-rc.5",
    "cross-env": "^7.0.3",
    "electron-squirrel-startup": "^1.0.0",
    "electron-winstaller": "^5.0.0",
    "font-awesome": "^4.7.0",
    "fontawesome": "^5.6.3",
    "glob": "^7.1.6",
    "jquery": "^3.6.0",
