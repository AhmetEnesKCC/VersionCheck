{
  "name": "version-check",
  "version": "1.0.0",
  "description": "check version of popular packages",
  "main": "out/electron/main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "start-d": "cross-env NODE_ENV=development nodemon --exec npm run start",
    "start-p": "cross-env NODE_ENV=production nodemon --exec npm run start",
    "package-windows": "electron-packager . --platform=win32 --arch=x64 --icon=build-contents/favicon.ico version-check"
  },
  "keywords": [
    "version",
    "check"
  ],
